{%- capture body_classes -%}
template-{{ template.name | handle }} {{ template.name }}-{{ template.suffix }} {{ template.name | append: '-' | append: handle }} {% if cart.item_count == 0 %}cart-empty{% endif %}
{%- endcapture -%}

{% liquid
  assign enable_rtl = settings.enable_rtl
  assign language_support_rtl = settings.language_support_rtl
  if enable_rtl and language_support_rtl != blank
    assign shop_locale = localization.language.iso_code
    assign language_support_rtl_list = language_support_rtl | split: ','
    assign is_rtl = language_support_rtl_list | where: shop_locale
    if is_rtl.size > 0
      assign enable_rtl = true
    else
      assign enable_rtl = false
    endif
  endif
%}
<!doctype html>
<html
  class="no-js supports-no-cookies"
  lang="{{ request.locale.iso_code }}"
  data-template="{{ template.name }}"
  {% if enable_rtl %}
    dir="rtl"
  {% endif %}
>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=0">
    <meta name="theme-color" content="{{ settings.color_primary }}">
    <meta name="google-site-verification" content="CZuHnrciDx5WXF9xPaj84B0WOnJdRbv3iATXhwz3d_s">

    {% if request.path == '/collections' %}
      <title>Geckobrands | Waterproof Gears | Collections</title>
      <meta
        name="description"
        content="Explore our premium collection of outdoor waterproof backpacks and gear, perfect for fishing, camping, jet skiing, hiking, and more at Geckobrands. Shop online today and enjoy great discounts!"
      >
    {% endif %}

    {% if handle contains 'all-products' %}
      <meta name="robots" content="noindex">
    {% endif %}

    {%- liquid
      render 'preload'
      render 'SEO'
      render 'social-meta-tags'
      render 'font-face'
    -%}

    {% render 'critical-css' %}
    {% render 'critical-scripts' %}
    {% render 'custom-code-head' %}

    <link rel="stylesheet" href="{{ 'theme.css' | asset_url }}">
    <link rel="stylesheet" href="{{ 'chunk.css' | asset_url }}">
    <link rel="stylesheet" href="{{ 'owl.theme.default.min.css' | asset_url }}">
    <link rel="stylesheet" href="{{ 'owl.carousel.min.css' | asset_url }}">
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.0/css/all.min.css"
      integrity="sha256-h20CPZ0QyXlBuAw7A+KluUYx/3pK+c7lYEpqLTlxjYQ="
      crossorigin="anonymous"
      media="print"
      onload="this.media='all'"
    >

    {% render 'style-tags' %}
    <script src="{{ 'jquery.min.js' | asset_url }}"></script>
    <script src="{{ 'theme-global.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'product-form.min.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'lazy-image.min.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'owl.carousel.min.js' | asset_url }}"></script>
    <script src="https://unpkg.com/swiper/swiper-bundle.min.js" defer="defer"></script>
    <script src="{{ 'jquery.lazyload.min.js' | asset_url }}" defer="defer"></script>

    <!-- starapps_scripts_start -->
    <!-- This code is automatically managed by StarApps Studio -->
    <!-- Please contact support@starapps.studio for any help -->
    <!-- starapps_scripts_end -->
    {{ content_for_header }}

    {% render 'judgeme_core' %}

    {% render 'digismoothie-giftbox' %}

    {% assign noindex_collections = 'frontpage,homepageslider,holiday-deals,2-compartment-coolers,soft-side-coolers,11-bottle-removable-hard-liner-coolers,new-items,more-new-items,tech,shop-by-category,beach-totes-1,test,waterproof-1,americana,cinch-sling-bags,cyber-week-sale,gym,other,rtc-coolers,shop-by-category,test'
      | split: ','
    %}

    {% if noindex_collections contains collection.handle %}
      <meta name="robots" content="noindex">
    {% endif %}

    <script type="text/javascript">
      (function(c,l,a,r,i,t,y){
        c[a]=c[a]||function(){(c[a].q=c[a].q||[]).push(arguments)};
        t=l.createElement(r);t.async=1;t.src="https://www.clarity.ms/tag/"+i;
        y=l.getElementsByTagName(r)[0];y.parentNode.insertBefore(t,y);
      })(window, document, "clarity", "script", "jobwyl8zqn");
    </script>
  </head>

  <body
    id="sf-theme"
    class="{{ body_classes }} relative min-h-screen{% if customer %} customer-logged-in{% endif %}"
    {% if template.name == 'product' %}
      data-product-id="{{ product.id }}"
    {% endif %}
  >
    {% render 'theme-data' %}
    {% section 'annoucement' %}
    {% section 'header' %}
    {% render 'scroll-top-button' %}

    <main role="main" id="MainContent">
      {{ content_for_layout }}
      <div id="cart-drawer-container"></div>
    </main>

    {%- liquid
      section 'footer'
    -%}

    {% section 'mobile-sticky-bar' %}

    {% render 'custom-code-body' %}
    {% render 'foxkit-messenger' %}
    {% render 'script-tags' %}

    <script src="{{ 'app.min.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'foxkit-app.min.js' | asset_url }}" defer="defer"></script>

    <script src="{{ 'quick-view.min.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'product-card-swatch.min.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'product-card-custom.js' | asset_url }}" defer="defer"></script>

    <div data-rebuy-id="91036"></div>
    <!-- {% render 'quantity-breaks-now', customer: customer, template: template, product: product, cart: cart %} -->

   
  
  </body>

  <script>
    window.onload = () => {
      document.getElementsByClassName('jdgm-preview-badge')[0].onclick = fun;
    };
    function fun() {
    document.getElementsByClassName('jdgm-revs-tab-btn')[0].click();
    }
  </script>
  <style>
    .jdgm-histogram-wrapper{display : none !important;}
      .jdgm-rev__icon{display : none !important;}
      .jdgm-prev-badge__text{cursor: pointer !important;}
      .jdgm-histogram{display : none !important;}
  </style>
</html>
