      var targetDate = new Date('2024-08-14T23:59:00-04:00').getTime();       
     
    function startCountdown() {
        var countdownInterval = setInterval(function() { 
            var currentDate = new Date().getTime();
            var timeRemaining = targetDate - currentDate;

            if (timeRemaining >= 0) {
                var days = Math.floor(timeRemaining / (1000 * 60 * 60 * 24));
                var hours = Math.floor((timeRemaining % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
                var minutes = Math.floor((timeRemaining % (1000 * 60 * 60)) / (1000 * 60));
                var seconds = Math.floor((timeRemaining % (1000 * 60)) / 1000);

                days = days < 10 ? '0' + days : days;
                hours = hours < 10 ? '0' + hours : hours;
                minutes = minutes < 10 ? '0' + minutes : minutes;
                seconds = seconds < 10 ? '0' + seconds : seconds;

                function createDigitDivs(number) {
                    return String(number).split('').map(digit => `<div class='digit'>${digit}</div>`).join('');
                } 

                $('#countdowns').html(
                    "<div class='box days'>" + createDigitDivs(days) + "</div>" +
                    "<div class='space'>:</div>" +
                    "<div class='box hours'>" + createDigitDivs(hours) + "</div>" +
                    "<div class='space'>:</div>" +
                    "<div class='box minutes'>" + createDigitDivs(minutes) + "</div>" +
                    "<div class='space'>:</div>" +
