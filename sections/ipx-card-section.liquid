{% stylesheet %}
  .ipx-card-rating{
    display:flex;
  }
.ipx-card-rating .ipx-rating-subdiv2
  {
   background:#f1f1f1;
    border:0.5px #f1f1f1;
    padding: 20px;
    border-radius: 20px;
    margin:20px 10px;
}
  .ipx-card-rating .ipx-rating-subdiv2 .ipx-icon{
    display:flex;
    margin:20px 0;
  }
    .ipx-card-rating .ipx-rating-subdiv2 .ipx-icon img{
      width:70px;
      height:70px;
      padding:5px;
    }
   .ipx-card-rating .ipx-rating-subdiv2 .ipx-icon p{
     line-height:1.2;
     text-align:left;
   }


{% endstylesheet %}

