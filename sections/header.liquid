{% javascript %}
  $('.mobile-megemenu-inner h2').click(function(j) {
          var dropDown = $(this).closest('.mobile-megemenu-inner').find('.mobile-megemenu-content'); 
          $(this).closest('.mobile-megemenu-custom').find('.mobile-megemenu-content').not(dropDown).slideUp();
          if ($(this).hasClass('active')) {
            $(this).removeClass('active'); 
          } else {
            $(this).closest('.mobile-megemenu-custom').find('h2.active').removeClass('active');
            $(this).addClass('active');
          }
          dropDown.stop(false, true).slideToggle();
          j.preventDefault();
      });
{% endjavascript %}
