                                  if show_compare_icon
                                    render 'header-option-item__compare', display_by: 'icon'
                                  endif

                                  if show_cart_icon
                                    render 'cart'
                                  endif
                                %}
                            </div>
                        </div>
                    </div>
                    {% assign box_size = container | append: ' -mx-4' %}
                    <div class="sf-menu-bar bg-color-menubar-background text-color-menubar">
